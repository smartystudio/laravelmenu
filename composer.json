{
  "name": "smartystudio/laravelmenu",
  "description": "Drag and drop menu generator for Laravel.",
  "homepage": "https://smartycms.net",
  "keywords": [
    "admin",
    "cms",
    "crud",
    "laravel",
    "menu",
    "bootstrap",
    "builder"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Smarty Studio MBN Ltd.",
      "homepage": "https://smartystudio.net",
      "email": "office@smartystudio.net"
    },
    {
      "name": "Martin Nestorov",
      "email": "m.nestorov@smartystudio.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "issues": "https://smartycms.net/issues"
  },
  "autoload": {
    "classmap": [
        "src/database"
    ],
    "psr-4": {
      "SmartyStudio\\LaravelMenu\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.* || 6.* || 7.* || 8.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SmartyStudio\\LaravelMenu\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "SmartyStudio\\LaravelMenu\\Facades\\Menu"
      }
    }
  }
}
